{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "typeRoots": ["node_modules/@types", "src/types"],
    "types": ["jest"],
    "baseUrl": "./src/",   
    "paths": {
        "@assets/*": ["assets/*"], 
        "@components/*": ["components/*"], 
        "@features/*": ["features/*"], 
        "@service/*": ["service/*"], 
        "@navigation/*": ["navigation/*"], 
        "@state/*": ["state/*"], 
        "@styles/*": ["styles/*"], 
        "@types/*": ["types/*"], 
        "@utils/*": ["utils/*"], 
    },
  }
}
